         sNomFichier est une chaine 		// Chemin de la vidéo
         
         SI gbCaptureEnCours = Vrai ALORS
         	Info("Un enregistrement est en cours")
         	RETOUR
         FIN
         
         // Sélection du fichier contenant la vidéo
         sNomFichier= fSélecteur("", "", "Ouvrir une vidéo", "Vidéo (*.avi)"+TAB+"*.avi"+RC+"Tous fichiers (*.*)"+TAB+"*.*", "avi", fselOuvre+fSelExiste)
         
