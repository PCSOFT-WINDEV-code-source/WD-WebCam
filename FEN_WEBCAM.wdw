        internal_properties : CAAAAAgAAABMlet5dcUCeVY0GFy2J92SpQKU3rYas3dF8g0rnxvZJQLsgxmhI0Uby5xSUYfPnk0JuaFKAraaRHw74FC+z07UOI+xc3wkoICnUoZ6pqLzHgL2W5CreIBcGiVyppK+yybtk60N8WTig/jY5sfxOqUtxmvAcfu5IyJ72fPFcUtwed7ZgsWmF+exF1OnnLfan4odXiiUyb3iNCEuqGeJUR8VIuRqCXYTGybc
        code : |1-
         SI gbCaptureEnCours = Vrai ALORS
         	Info("Un enregistrement est en cours")
         	RETOUR
         FIN
         
         // Sauvegarde de la vidéo retransmise dans le champ Vidéo 
         si pas VidéoCapture(CAM_VIDEO,CompleteRep(fRepExe())+"Temp.bmp",viCaptureImage) alors
         	erreur(ErreurInfo())
         	retour
         FIN
         
         // Récupération du dernier répertoire utilisé
         Si gsRepDernierCapture~="" alors
         	gsRepDernierCapture = fRepEnCours()
         FIN
         
         // Renomme le fichier AVI
         sNomFichier est une chaine = fSélecteur(gsRepDernierCapture, "", "Enregistrer sous ...", "Image (*.bmp)"+TAB+"*.bmp"+RC+"Tous fichiers (*.*)"+TAB+"*.*", "bmp", fSelCrée+fSelExiste)
         si sNomFichier~="" alors
         	// Suppression du fichier temporaire
         	fsupprime(CompleteRep(fRepExe())+"Temp.bmp")
         sinon
         	// Renommage et déplacement du fichier généré
         	Si sNomFichier~=CompleteRep(fRepExe())+"Temp.bmp" alors
         		// Le fichier est déjà le bon
         	SINON
         		// Si un fichier du même nom que le fichier destination existe, on le supprime
         		Si fFichierExiste(sNomFichier) alors fSupprime(sNomFichier)
         		// On renomme le fichier temporaire dans le nom sélectionné
         		fRenomme(CompleteRep(fRepExe())+"Temp.bmp",sNomFichier)	
         	FIN
         	
         	// Mémorisation du répertoire
         	gsRepDernierCapture = fExtraitChemin(sNomFichier,fDisque+fRépertoire)
         FIN
         
         CAS EXCEPTION:
         	sMsgErr est une chaîne = ErreurInfo()
         	SI sMsgErr~="" ALORS sMsgErr="La WebCam ne répond pas."+RC+"Vérifiez qu'elle est correctement branchée et que son driver est bien installé."
         	Erreur(sMsgErr)
         	RepriseSaisie()
