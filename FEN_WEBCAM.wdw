     type_code : 14
     code : |1-
      // Résumé : Chargement de la configuration en mode "serveur"
      // Syntaxe :
      //ChargementConfiguration ( [<bModificationEnCours> est booléen])
      //
      // Paramètres :
      // 	bModificationEnCours (booléen - valeur par défaut=0) : Vrai pour les modifications en cours de fonctionnement
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE ChargementConfiguration(bModificationEnCours est un booléen = Faux)
      
      // Arrêt du timer automatique
      SI NumTimerServeur>0 alors FinTimerSys(NumTimerServeur)
      
      // Lecture du chemin de l'image dans le .INI
      NomImage=INILit("Parametres", "Image", "",FichierINI)
