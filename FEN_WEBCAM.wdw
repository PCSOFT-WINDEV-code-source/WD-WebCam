      // Lecture de la fréquence dans le .INI
      Frequence=INILit("Parametres", "Frequence", "", FichierINI)
      SI Frequence=0 ALORS Frequence=100
      
      // Lecture de l'iconisation dans le .INI
      IconiserLancement=INILit("Parametres", "Iconiser", "", FichierINI)
      
      // Lecture du mode "serveur" dans le .INI
      INT_SansNom1[1]=INILIT("Parametres","Serveur","0",FichierINI)
      SI INT_SansNom1[1] = Vrai alors
      	// Lancement du timer automatique
      	NumTimerServeur=TimerSys(ActualisationServeur,Frequence)
      FIN
      
      // Gestion de l'iconisation seulement au chargement
      SI bModificationEnCours = Faux alors
      	SI IconiserLancement ALORS Iconise()
      FIN
