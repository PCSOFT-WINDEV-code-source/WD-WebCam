         si sNomFichier="" alors retour
         
         // Mise en place des champs
         CAM_VIDEO..visible=faux 				//champ de la caméra
         GRPBTNCAMERA..visible=faux 				//boutons d'enregistrement de la caméra
         MM_VISUALISATION..Visible = Vrai 		//champ de visualisation
         BTN_ARRETER..visible=vrai 				//bouton pour arrêter
         GRPBTNVISU..visible=faux				//boutons de visualisation des séquences vidés ou captures
         
         // Ouverture de l'animation
         MM_VISUALISATION..Visible = Vrai
         SI MultimédiaJoue(MM_VISUALISATION,sNomFichier) = Faux alors
         	erreur("Erreur de lecture",erreurinfo(errMessage))
         FIN
         
         // On attend la fin de la vidéo
         tantque MultimédiaEtat(MM_VISUALISATION)=MultimédiaEtatJoue
         	multitache(-5)
         fin
         
         // Libération du lecteur multimédia
         MultimédiaFerme(MM_VISUALISATION)
         
         GRPBTNCAMERA..visible=vrai //boutons d'enregistrement de la caméra
         MM_VISUALISATION..Visible = Faux//champ de visualisation
